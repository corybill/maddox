{
  "name": "maddox",
  "version": "0.0.5",
  "description": "BDD style Scenario Unit Testing to test functionality and performance.",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run ut && npm run lint",
    "ut": "node ./bin/maddox ./spec/unit",
    "perf": "env perf=true env test=false maddox ./spec/unit",
    "uap": "env perf=true test=true maddox ./spec/unit",
    "coverage": "./bin/maddox-cov.js",
    "lint": "eslint ./spec/ ./lib"
  },
  "bin": {
    "maddox": "./bin/maddox.js"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corybill/maddox.git"
  },
  "keywords": [
    "Scenario Testing",
    "BDD",
    "Behavior Driven Development",
    "BDD Unit Testing",
    "BDD Integration Testing"
  ],
  "author": "Cory Parrish",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/corybill/maddox/issues"
  },
  "homepage": "https://github.com/corybill/maddox#readme",
  "contributors": [
    {
      "name": "Kuntal Roy"
    },
    {
      "name": "Meir Rosner"
    },
    {
      "name": "Daniel Kim"
    }
  ],
  "dependencies": {
    "benchmark": "^2.1.0",
    "bluebird": "^3.3.4",
    "chai": "3.5.0",
    "fs-extra": "^0.26.5",
    "lodash": "^4.6.1",
    "mocha": "2.4.5",
    "sinon": "1.17.3"
  },
  "devDependencies": {
    "chance": "^1.0.1",
    "eslint": "^2.3.0",
    "istanbul": "^0.4.2",
    "preconditions": "^1.0.8"
  }
}
